{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@utilities/*": [
        "src/utilities/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@public/*": [
        "public/*"
      ]
    }
  }
}
